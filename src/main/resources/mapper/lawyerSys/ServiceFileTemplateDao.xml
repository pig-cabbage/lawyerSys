<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cabbage.project.lawyerSys.dao.ServiceFileTemplateDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="cabbage.project.lawyerSys.entity.ServiceFileTemplateEntity" id="serviceFileTemplateMap">
        <result property="id" column="id"/>
        <result property="fileName" column="file_name"/>
        <result property="url" column="url"/>
        <result property="type" column="type"/>
        <result property="suffix" column="suffix"/>
        <result property="parent" column="parent"/>
        <result property="plan" column="plan"/>
    </resultMap>
    <update id="setPlan">
        UPDATE `service_file_template` SET `plan`=#{planId} WHERE `plan`=#{userInfo}
    </update>


</mapper>