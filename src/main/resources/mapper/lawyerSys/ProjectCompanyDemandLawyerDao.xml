<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cabbage.project.lawyerSys.dao.ProjectCompanyDemandLawyerDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="cabbage.project.lawyerSys.entity.ProjectCompanyDemandLawyerEntity"
               id="projectCompanyDemandLawyerMap">
        <result property="id" column="id"/>
        <result property="project" column="project"/>
        <result property="recommendLawyer" column="recommend_lawyer"/>
        <result property="demandLawyer" column="demand_lawyer"/>
        <result property="createTime" column="create_time"/>
    </resultMap>
    <select id="getProjectLatest"
            resultType="cabbage.project.lawyerSys.entity.ProjectCompanyDemandLawyerEntity">
        SELECT * FROM `project_company_demand_lawyer` WHERE `project` = #{projectId} ORDER BY `create_time` DESC LIMIT 0, 1
    </select>


</mapper>