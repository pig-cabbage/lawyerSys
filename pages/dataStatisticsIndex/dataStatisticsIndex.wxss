/* pages/dataStatisticsIndex/dataStatisticsIndex.wxss */
page{
  width: 100%;
  height : 100%;
}
.page{
  width: 100%;
  height : 100%;
}
.time{
  width: 100%;
  height: 100rpx;
  margin : 10rpx auto;
  line-height: 50rpx;
  display:flex;
}
.time-text{
  width: 35%;
  font-size:40rpx;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
  text-align: center;
}
.time-body{
  font-size:43rpx;
}
.object{
  width: 100%;
  height: 100rpx;
  margin : 10rpx auto;
  line-height: 50rpx;
  display:flex;
}
.object-body{
  font-size:43rpx;
  display: flex;
}
.nav_wrap{
  align-items: center;
}
.nav{
  /* white-space: nowrap; */
  padding: 5rpx;
}
.nav_item{
  padding: 15rpx 30rpx;
  font-size: 40rpx;
  display: inline-block;
}
/*粗度5rpx 实线 粉色*/
.nav_item.active{
  border-bottom: 5rpx solid #1E90FF;
}

.table {
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 90%;
  justify-content: center;
  height: 3rem;
  align-items: center;
  margin : 0 auto;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
  justify-content: center;
  background: #3366FF;
  color: #fff;
  /* display: flex; */
  height: 3rem;
  line-height: 3rem;
  text-align: center;
}
.th-top{
  width: 100%;
  justify-content: center;
  color: #fff;
  /* display: flex; */
  height: 2rem;
  line-height: 2rem;
  text-align: center;
}
.th-bottom{
  width: 100%;
  justify-content: center;
  color: #fff;
  /* display: flex; */
  height: 1rem;
  line-height: 1rem;
  text-align: center;
}